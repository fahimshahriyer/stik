{
  "name": "stik",
  "version": "1.0.0",
  "description": "A simple frontend boilerplate for static project using Gulp, Pug and Sass",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LFeh/kratos-boilerplate.git"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "scripts": {
    "start": "gulp",
    "test": "npm run lint && npm run reporter",
    "setup": "npm i && gulp build",
    "fix": "./node_modules/.bin/eslint ./src/ --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "./node_modules/.bin/eslint ./src/",
    "lint:css": "./node_modules/.bin/stylint ./src/",
    "reporter": "./node_modules/.bin/parker ./build/css/*.css",
    "precommit": "npm run lint",
    "build": "gulp build",
    "html": "gulp html",
    "js": "gulp js",
    "css": "gulp css",
    "images": "gulp images",
    "icons": "gulp icons",
    "svg": "gulp svg"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Fahim Shahriyer <shahriyers@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LFeh/kratos-boilerplate/issues"
  },
  "homepage": "https://github.com/LFeh/kratos-boilerplate#readme",
  "dependencies": {
    "lazysizes": "^4.0.0-rc3",
    "picturefill": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "browser-sync": "^2.18.12",
    "del": "^3.0.0",
    "eslint": "^4.2.0",
    "fs-promise": "^2.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-data": "^1.2.1",
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.3.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-sass": "^3.1.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.0",
    "gulp-uglify": "^3.0.0",
    "husky": "^0.14.3",
    "js-yaml": "^3.9.0",
    "mdcss": "^1.5.2",
    "parker": "0.0.10",
    "stylint": "^1.5.9"
  }
}
